  author={Li, Yang and Kan, Shichao and Wenming, Cao and He, Zhihai},
  journal={IEEE Transactions on Circuits and Systems for Video Technology}, 
  title={Learned Model Composition With Critical Sample Look-Ahead for Semi-Supervised Learning on Small Sets of Labeled Samples}, 
