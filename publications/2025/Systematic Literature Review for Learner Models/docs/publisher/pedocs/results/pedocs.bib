@article{ Jivet2020,
author = {Ioana Jivet and Maren Scheffel and Marcel Schmitz and Stefan Robbers and Marcus Specht and Hendrik Drachsler},
title = {From students with love. An empirical study on learner goals, self-regulated learning and sense-making of learning analytics in higher education},
year = {2020},
journal = {The internet and higher education},
publisher = {Elsevier, Jai Press},
address = {New York, NY ; Greenwich, Conn. },
volume = {47},
number = {100758},
keywords = {E-Learning; Datenanalyse; Selbstgesteuertes Lernen; Sinngebung; Tools; Lernerorientierung; Lernziel; Hochschulbildung; Student; Empirische Untersuchung; Mixed-Methods-Design; Qualitative Forschung; Quantitative Forschung;
E-learning; Virtual learning; Teaching objective; Higher education; University level of education; Male student; Empirical study; Qualitative research; Quantitative research},
abstract = {Unequal stakeholder engagement is a common pitfall of adoption approaches of learning analytics in higher education leading to lower buy-in and flawed tools that fail to meet the needs of their target groups. With each design decision, we make assumptions on how learners will make sense of the visualisations, but we know very little about how students make sense of dashboard and which aspects influence their sense-making. We investigated how learner goals and self-regulated learning (SRL) skills influence dashboard sense-making following a mixed-methods research methodology: a qualitative pre-study followed-up with an extensive quantitative study with 247 university students. We uncovered three latent variables for sense-making: transparency of design, reference frames and support for action. SRL skills are predictors for how relevant students find these constructs. Learner goals have a significant effect only on the perceived relevance of reference frames. Knowing which factors influence students' sense-making will lead to more inclusive and flexible designs that will cater to the needs of both novice and expert learners. (DIPF/Orig.)},
issn = {1096-7516},
urn = {https://nbn-resolving.org/urn:nbn:de:0111-pedocs-232214},
doi = {https://doi.org/10.25656/01:23221},
doi = {https://doi.org/10.1016/j.iheduc.2020.100758},
language = {eng},
copyright = {http://creativecommons.org/licenses/by-nc-nd/4.0/deed.de},
}

