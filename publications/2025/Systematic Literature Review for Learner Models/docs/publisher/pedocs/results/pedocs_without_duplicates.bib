@incollection{henning_personalized_2014,
	title = {Personalized web learning by joining {OER}},
	copyright = {https://creativecommons.org/licenses/by-nc-nd/3.0/at/deed.de},
	isbn = {978-3-88579-627-5},
	abstract = {We argue that quality issues and didactical concerns of MOOCs may be overcome by relying on small Open Educational Resources, joining them into concise courses by gluing them together along predefined learning pathways with proper semantic annotations. This new approach to adaptive learning does not attempt to model the learner, but rather concentrates on the learning process and established models thereof. Such a new approach does not only require conceptual work and corresponding support tools, but also a new meta data format and an engine which may interpret the semantic annotations as well as measure a learner’s response to these. The EU FP7 project INTUI℡ is introduced, which employs these technologies in a novel learning environment.},
	language = {eng},
	booktitle = {{DeLFI} 2014. {Die} 12. e-{Learning} {Fachtagung} {Informatik} der {Gesellschaft} für {Informatik} e.{V}., 15.-17. {September} 2014 in {Freiburg}},
	author = {Henning, Peter A. and Fuchs, Kevin and Bock, Jürgen and Zander, Stefan and Steicher, Alexander and Zielinski, Andrea and Swertz, Christian and Forstner, Alexandra and Badii, Atta and Thiemert, Daniel and Perales, Oscar Garcia},
