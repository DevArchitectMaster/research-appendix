	keywords = {Acquisition of knowledge, Educational game, Einfluss, Fiction, Fiktion, Geschichte, History, Identification, Identifikation, Influence, Instructional game, Learning achievement, Lernender, Lernerfolg, Lernspiel, Motivation, Objektivität, Playing, Realität, Reality, Spiel, Studie, Subjectivity, Subjektivität, Vergleichsuntersuchung, Wissensgenerierung},
